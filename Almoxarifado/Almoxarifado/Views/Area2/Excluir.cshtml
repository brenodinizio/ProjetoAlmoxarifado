@model Almoxarifado.Models.AREA
@{
    ViewBag.Title = "Create";
}

<h2>Excluindo Área</h2>

<div>
    <form action="/Area2/ExcluirConfirmar?id=@Model.AREID" method="post">
        <div>
            <label>Código: </label>

            <label>@Model.AREID </label>
        </div>

        <div>
            <label>Descrição: </label>

            <label>@Model.AREDESCRICAO </label>
        </div>
        
       
        <button class="btn btn-danger" type="submit">Excluir</button>
        <a href="/Area2/Index" class="btn btn-primary">Voltar</a>

    </form>


</div>